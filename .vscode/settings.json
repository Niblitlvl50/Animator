{
    "python.linting.pylintEnabled": false,

    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.vscode": false,
        "**/*.pyc": true,
        "**/*.VC.db": true,
        "bin/": true,
        "cmake_install.cmake": true,
        "CMakeCache.txt": true,
        "CMakeFiles": true,
        "Makefile": true
    },
    "files.associations": {
        "ios": "cpp",
        "array": "cpp",
        "iterator": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "type_traits": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "bitset": "cpp",
        "initializer_list": "cpp",
        "map": "cpp",
        "string_view": "cpp",
        "unordered_map": "cpp",
        "utility": "cpp",
        "valarray": "cpp"
    }
}